{
    "name": "siu/queue",
    "license": "Apache License 2.0",
    "description": "Queue简易框架",
    "type": "project",
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "7.3"
        }
    },
    "autoload": {
        "psr-4": {
            "Siu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Siu\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3",
        "symfony/dotenv": "^5.0",
        "symfony/http-client": "^5.0",
        "wisembly/elephant.io": "^3.3"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.0",
        "phpunit/phpunit": "^8"
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
